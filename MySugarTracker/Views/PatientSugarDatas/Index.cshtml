@model IEnumerable<MySugarTracker.Models.PatientSugarData>

@{
    ViewBag.Title = "Glucose Reading History";
}

<h2 align="center">Glucose Reading History</h2>

<p>
    @Html.ActionLink("New Glucose Reading", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.patientSugarReading)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.patientSugarReading)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PatientSugarDataID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PatientSugarDataID })
        </td>
    </tr>
}
 
</table>
<div><p><a href="../Patients/Index">Back to Patient Profile &raquo;</a></p></div>